// --------------------------------------------------------
// Sector Performance Models
// --------------------------------------------------------

model SectorPerformance {
  id                 Int      @id @default(autoincrement())
  date               DateTime @db.Date
  sectorCode         String   // Sector identifier (e.g., 'XLK', 'XLF', etc.)
  sectorName         String   // Sector name (e.g., 'Technology', 'Financials')
  closePrice         Decimal  @db.Decimal(12, 4)
  openPrice          Decimal? @db.Decimal(12, 4)
  highPrice          Decimal? @db.Decimal(12, 4)
  lowPrice           Decimal? @db.Decimal(12, 4)
  volume             BigInt?
  change             Decimal? @db.Decimal(12, 4) // Price change
  changePercent      Decimal? @db.Decimal(8, 4)  // Percentage change
  yearToDateReturn   Decimal? @db.Decimal(8, 4)  // YTD return %
  oneMonthReturn     Decimal? @db.Decimal(8, 4)  // 1-month return %
  threeMonthReturn   Decimal? @db.Decimal(8, 4)  // 3-month return %
  oneYearReturn      Decimal? @db.Decimal(8, 4)  // 1-year return %
  marketCap          BigInt?  // Total market cap of sector
  averagePE          Decimal? @db.Decimal(8, 2)  // Average P/E ratio
  averagePB          Decimal? @db.Decimal(8, 2)  // Average P/B ratio
  dividendYield      Decimal? @db.Decimal(6, 4)  // Average dividend yield
  createdAt          DateTime @default(now())
  updatedAt          DateTime @updatedAt

  @@unique([date, sectorCode])
  @@index([date])
  @@index([sectorCode])
  @@index([sectorName])
  @@map("sector_performance")
}

model SectorComparison {
  id                 Int      @id @default(autoincrement())
  date               DateTime @db.Date
  comparisonType     String   // Type of comparison (e.g., 'relative_strength', 'volatility')
  sectorCode         String   // Sector being compared
  benchmarkCode      String   // Benchmark (e.g., 'SPY' for S&P 500)
  relativeStrength   Decimal? @db.Decimal(8, 4) // Sector vs benchmark performance
  beta               Decimal? @db.Decimal(6, 4) // Sector beta
  correlation        Decimal? @db.Decimal(6, 4) // Correlation with benchmark
  volatility         Decimal? @db.Decimal(8, 4) // Historical volatility
  sharpeRatio        Decimal? @db.Decimal(8, 4) // Risk-adjusted return
  rank               Int?     // Ranking among sectors
  createdAt          DateTime @default(now())
  updatedAt          DateTime @updatedAt

  @@unique([date, comparisonType, sectorCode, benchmarkCode])
  @@index([date])
  @@index([sectorCode])
  @@index([comparisonType])
  @@map("sector_comparison")
}

// --------------------------------------------------------
// Global Asset Comparison Models
// --------------------------------------------------------

model GlobalAssetPerformance {
  id                 Int      @id @default(autoincrement())
  date               DateTime @db.Date
  assetCode          String   // Asset ticker (e.g., 'GC=F', 'BTC-USD', '^GSPC')
  assetName          String   // Asset name (e.g., 'Gold', 'Bitcoin', 'S&P 500')
  category           String   // Category: commodity, index, crypto, forex, dollar_index, real_estate
  closePrice         Decimal  @db.Decimal(16, 4)
  openPrice          Decimal? @db.Decimal(16, 4)
  highPrice          Decimal? @db.Decimal(16, 4)
  lowPrice           Decimal? @db.Decimal(16, 4)
  volume             BigInt?
  change             Decimal? @db.Decimal(16, 4) // Price change
  changePercent      Decimal? @db.Decimal(8, 4)  // Percentage change
  yearToDateReturn   Decimal? @db.Decimal(8, 4)  // YTD return %
  oneMonthReturn     Decimal? @db.Decimal(8, 4)  // 1-month return %
  threeMonthReturn   Decimal? @db.Decimal(8, 4)  // 3-month return %
  sixMonthReturn     Decimal? @db.Decimal(8, 4)  // 6-month return %
  oneYearReturn      Decimal? @db.Decimal(8, 4)  // 1-year return %
  volatility         Decimal? @db.Decimal(8, 4)  // 30-day volatility
  createdAt          DateTime @default(now())
  updatedAt          DateTime @updatedAt

  @@unique([date, assetCode])
  @@index([date])
  @@index([assetCode])
  @@index([category])
  @@map("global_asset_performance")
}

model NormalizedAssetData {
  id                 Int      @id @default(autoincrement())
  baseDate           DateTime @db.Date // Start date for normalization (value = 100)
  currentDate        DateTime @db.Date // Current data point date
  assetCode          String   // Asset ticker
  assetName          String   // Asset name
  category           String   // Category
  normalizedValue    Decimal  @db.Decimal(12, 4) // Normalized to 100 at baseDate
  actualPrice        Decimal  @db.Decimal(16, 4) // Actual price
  percentChange      Decimal  @db.Decimal(8, 4)  // % change from base
  createdAt          DateTime @default(now())
  updatedAt          DateTime @updatedAt

  @@unique([baseDate, currentDate, assetCode])
  @@index([baseDate])
  @@index([currentDate])
  @@index([assetCode])
  @@index([category])
  @@map("normalized_asset_data")
}

model AssetCorrelation {
  id                 Int      @id @default(autoincrement())
  date               DateTime @db.Date
  asset1Code         String   // First asset
  asset2Code         String   // Second asset
  correlation        Decimal  @db.Decimal(6, 4) // Correlation coefficient (-1 to 1)
  period             Int      // Period in days (e.g., 30, 90, 365)
  createdAt          DateTime @default(now())
  updatedAt          DateTime @updatedAt

  @@unique([date, asset1Code, asset2Code, period])
  @@index([date])
  @@index([asset1Code])
  @@index([asset2Code])
  @@map("asset_correlation")
}

